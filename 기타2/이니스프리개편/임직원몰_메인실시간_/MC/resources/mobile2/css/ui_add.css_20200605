@charset "utf-8";

/* common 수정할것 */
.horizScroll{margin-top:2.5rem;margin-bottom: -4.5rem;}
.floatingBanner a{box-shadow: 2px 3px 4px 1px rgba(0,0,0,.2); border: 0;}
.floatingBanner .btnHide::before{background: #363636 url(../images/common/icon_exfloatingclose.png) no-repeat center;background-size: contain;}
header nav .headerNav > .list > li .ic-hot{position: absolute; top: 6px; right: 2px; width: 1.4rem; height: 1.4rem; background: url(../images/common/ic_hot.png) no-repeat;background-size: contain;}

