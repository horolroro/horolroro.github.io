	$(function() {
		/* INSTALOG와 OFFICIAL 속도개선으로 fl값이 Y일때만 실행하도록  */
		if("${dpTitleList[10].useFl}" == "Y"){
			instaLoad('INSTALOG', '1');
			instaLoad('OFFICIAL', '2');	
		}
	});
 
	function fncSaveJim(prdSeq, prdSeq2,myshopYn,myshopFl) {
		var flag = $.jcom.saveJim(prdSeq, prdSeq2,myshopYn,myshopFl);
    	if(flag != "false"){
    		wishSel(myshopYn,myshopFl);
    	}
    }
	
	function dynamicRank(obj){
		if($(obj).parent('li').hasClass('active'))return;
		var target = $(obj).data('target');		
		$('.rankingTab').find('li').removeClass('active');
		$(obj).parent('li').addClass('active');
		//$(obj).addClass('active');
		
		$.ajax({
			url : '/kr/ko/mMainOnlineSalRank.do',
			type : 'GET',
			data : {			
				viewType : target
			},
			dataType : "text",
			success : function(data){
				if(target == "view"){
					$(".cateWrap").hide();					
					$('#dynamicDiv').html(data);					
				}else if(target =="sale"){
					$(".cateWrap").show();
					var activeLeft = $('.cateWrap > ul > li:first').position().left- $('.cateWrap > ul > li:first').width();
					$(".cateWrap > ul").scrollLeft(activeLeft);
					$('.cateWrap li').removeClass('active');
					$('.cateWrap > ul > li:first').addClass('active');		
					$('#dynamicDiv').html(data);					
				}			
				onRanking.init();
			},
			error : function(data){
				$.jcom.showCommonLayer("처리 중 장애가 발생하였습니다. 잠시 후 재시도 후에도 지속적인 문제 발생 시 고객센터로 문의 바랍니다.");
			}
		});				
	}
	function dynamicRank2(ct){
		$.ajax({
			url : '/kr/ko/mMainOnlineSalRank.do',
			type : 'GET',
			data : {			
				catCd : ct
			},
			dataType : "text",
			success : function(data){
				$('#dynamicDiv').html(data);	
				onRanking.init()
			},
			error : function(data){
				$.jcom.showCommonLayer("처리 중 장애가 발생하였습니다. 잠시 후 재시도 후에도 지속적인 문제 발생 시 고객센터로 문의 바랍니다.");
			}
		});
	}

